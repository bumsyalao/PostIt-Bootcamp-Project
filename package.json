    "start": "babel-node -- server.js",
    "start-dev": "webpack --config ./webpack.config.js --progress --colors && nodemon --watch server --exec babel-node -- server.js",
    "coverage-report": "cat ./coverage/lcov.info | coveralls",
    "test": "NODE_ENV=test sequelize db:migrate && nyc --reporter=text --reporter=html --reporter=lcov mocha --compilers js:babel-core/register \"test/**/*.test.js\" --timeout 5000 && npm run coverage-report",
    "prebuild": "rm -rf ./dist",
    "build": "webpack --config ./webpack.config.prod.js --progress --colors && nodemon --watch server --exec node -- server.prod.js",
    "postbuild": "node server.prod.js"
  },
