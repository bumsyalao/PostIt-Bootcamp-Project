{
  "name": "postit-bootcamp-project",
  "version": "1.0.0",
  "description": "An app that allos users create groups and post messages to group.",
  "main": "app.js",
  "engines": {
    "node": "8.4.0",
    "npm": "5.3.0"
  },
  "scripts": {
    "client-test": "jest",
    "start": "babel-node -- server.js",
    "start-dev": "webpack --config ./webpack.config.js --progress --colors && nodemon --watch server --exec babel-node -- server.js",
    "postinstall": "npm run build",
    "coverage-report": "cat ./coverage/lcov.info | coveralls",
    "test": "NODE_ENV=test sequelize db:migrate && nyc --reporter=text --reporter=html --reporter=lcov mocha --compilers js:babel-core/register \"test/**/*.test.js\" --timeout 5000 && npm run coverage-report",
    "prebuild": "rm -rf ./dist",
    "build": "npm run build:server && npm run build:client",
    "postbuild": "node server.prod.js",
    "build:server": "babel server -d dist/server --copy-files --ignore test",
    "build:client": "NODE_ENV=production webpack --config webpack.config.prod.js && cp -R client dist/client"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bumsyalao/PostIt-Bootcamp-Project.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/bumsyalao/PostIt-Bootcamp-Project/issues"
  },
  "homepage": "https://github.com/bumsyalao/PostIt-Bootcamp-Project#readme",
  "dependencies": {
    "axios": "^0.16.2",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-react-html-attrs": "^2.0.0",
    "babel-plugin-transform-class-properties": "^6.3.13",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "bcrypt": "^1.0.2",
    "body-parser": "^1.17.2",
    "chai": "^4.0.2",
    "chai-http": "^3.0.0",
    "compression": "^1.7.0",
    "cross-env": "^5.0.2",
    "crypto-js": "^3.1.9-1",
    "css-loader": "^0.28.4",
    "dotenv": "^4.0.0",
    "enzyme": "^3.0.0",
    "enzyme-adapter-react-15": "^1.0.0",
    "enzyme-to-json": "^3.0.1",
    "express": "^4.15.4",
    "extract-text-webpack-plugin": "^3.0.0",
    "html-webpack-plugin": "^2.29.0",
    "jquery": "^3.2.1",
    "jsonwebtoken": "^7.4.1",
    "mocha": "^3.4.2",
    "morgan": "^1.8.2",
    "mysql": "^2.13.0",
    "nexmo": "^2.0.2",
    "node-sass": "^4.5.3",
    "nodemailer": "^4.0.1",
    "pg": "^6.1.5",
    "pg-hstore": "^2.3.2",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-addons-test-utils": "^15.6.0",
    "react-dom": "^15.6.1",
    "react-paginate": "^4.4.4",
    "react-redux": "^5.0.6",
    "react-router": "^4.1.2",
    "react-router-dom": "^4.1.2",
    "react-router-redux": "^4.0.8",
    "redux": "^3.7.2",
    "redux-mock-store": "^1.3.0",
    "redux-thunk": "^2.2.0",
    "sass-loader": "^6.0.6",
    "sequelize": "^3.30.4",
    "sequelize-cli": "^2.7.0",
    "sinon": "^4.0.1",
    "style-loader": "^0.18.2",
    "supertest": "^3.0.0",
    "webpack": "^3.5.2",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-hot-middleware": "^2.18.2"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "babel-jest": "^21.0.0",
    "babel-loader": "^7.1.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "copy-webpack-plugin": "^4.0.1",
    "coveralls": "^2.13.1",
    "ember-cli-materialize": "^0.22.0-beta.0",
    "enzyme-adapter-react-16": "^1.0.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx": "0.0.2",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.3.0",
    "expect": "^21.2.1",
    "file-loader": "^0.11.2",
    "img-loader": "^2.0.0",
    "jest": "^21.0.1",
    "jquery": "^3.2.1",
    "moxios": "^0.4.0",
    "nodemon": "^1.11.0",
    "nyc": "^11.1.0",
    "react-hot-loader": "^1.3.1",
    "react-test-renderer": "^16.0.0",
    "regenerator-runtime": "^0.11.0",
    "url-loader": "^0.5.9",
    "webpack-hot-middleware": "^2.18.2"
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/client/test-env.js"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/assetsTransformer.js",
      "\\.(css|less)$": "<rootDir>/assetsTransformer.js"
    },
    "roots": [
      "<rootDir>/client/"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!**/node_modules/**",
      "!**/vendors/**",
      "!**/public/**",
      "!**/server/**",
      "!**/coverage/**",
      "!webpack.config.js",
      "!bundle.js",
      "!**/client/Index.jsx",
      "!**/client/Routes.jsx",
      "!**/client/bundle.js",
      "!**/client/reducers/rootReducer.js",
      "!**/client/store/**",
      "!**/client/utils/**",
      "!assetsTransformer.js"
    ]
  }
}
